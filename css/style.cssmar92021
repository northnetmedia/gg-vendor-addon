	background-color: #abacb2;
    padding: 30px 0;
    position: relative; 
	margin: 100px 0;
}
.vendor-detail-slide-section:before{
	content: '';
	position: absolute;
	top: -10%;
	bottom: -10%;
	left: 0;
	right: 0;
	background-image: url(img/vendor-bg.png);
	background-repeat: no-repeat;
	background-size: cover; 
	height: 100%;
	width: 100%;
	z-index: -1
}
